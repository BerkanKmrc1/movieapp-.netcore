@model IEnumerable<Movie>

@foreach (var item in Model)
{
    <div class="card mb-3">
    <div class="row no-gutter">
        <div class="col-md-3">
            <img src="/lib/img/@item.ImageUrl" alt="" class="card-img">
        </div>
        <div class="col-md-9">
            <div class="card-body">
                <h5 class="card-title">
                    <a href="/Admin/Details/@item.Id">@item.CourseName</a>
                </h5>
                <p class="card-text">
                  @item.CourseCode
                </p>
                <p class="card-text">
                <small class="text-muted">3 days ago</small>
                </p>
            </div>
        </div>
    </div>
</div>
}


    